{
	"name": "bakeryreact",
	"version": "1.0.0",
	"description": "Bakery made with MERN stack",
	"main": "server.js",
	"scripts": {
		"client-install": "npm install --prefix client",
		"start": "node server.js",
		"server": "nodemon server.js",
		"client": "npm run build:css --prefix client && npm start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client \""
	},
	"author": "Charles Graham",
	"license": "MIT",
	"dependencies": {
		"axios": "^0.21.1",
		"concurrently": "^5.1.0",
		"express": "^4.17.1",
		"mongodb": "^3.5.5",
		"mongoose": "^5.9.7"
	},
	"devDependencies": {
		"nodemon": "^2.0.2"
	}
}
